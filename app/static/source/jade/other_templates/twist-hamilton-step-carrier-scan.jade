div(class="twst-hamilton-step")
    div("twst-hamilton-step-direction")
        span(class="twst-bullet-number") 2
        span Scan Carriers from Left to Right:
            span(class="twst-hamilton-carriers-remaining" ng-ig="scannedCarrierCount < hamiltonDataObj.allCarriers.length") {{hamiltonDataObj.allCarriers.length - scannedCarrierCount}} remaining
            button(class="twst-button twst-red-button twst-carriers-start-over-button" ng-click="restartCarrierScan()") start over